<div class="container-fluid">
  <div class="jumbotron">
    <%= form_for @post, url: {action: "create"} do |f|%>
      TITLE<%=f.text_field :title, class:"form-control"%><br>
      CONTENT<%=f.text_area :content, size: "60x15", class:"form-control" %>
      <br>
      <%=f.submit "작성", class:"mybutton devise btn-larger"%>
    <% end %>
    
    
    <% @post_pull.each do |p| %>
    <h3><%= p.title %></h3><br>
    <p><%= p.content %></p>
    <hr>
    <% end %>
  </div>
</div>